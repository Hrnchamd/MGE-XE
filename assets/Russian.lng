[MainForm.ToolTips]
tpDL = "Эта вкладка определяет, будут ли, и каким образом будут отображаться\nDistant Land, дальние объекты, анимированная трава и отражения на воде,\nа также содержит настройки погоды для Distant Land, такие как скорость\nветра и дальность тумана."
tpGlobal = "Содержит общие настройки графики, такие как разрешение экрана,\nчастота обновления, уровень сглаживания, формат буфера глубины."
tpINI = "Позволяет легко изменять некоторые настройки\nв файле Morrowind.ini из MGE."
tpInput = "Содержит параметры, влияющие на устройства ввода, а также\nредактор команд клавиатуры и переназначение клавиш."
tpMain = "Содержит общие настройки и кнопки быстрого вызова для\nизменения множества настроек одним щелчком мыши."
tpMisc = "Содержит настройки, которые не подходят для других\nвкладок, в том числе настройка, отключающая MWSE."
tpRender = "Содержит множество настроек, влияющих на способ\nсоздания графики в игре Morrowind."
tpTools = "Содержит редактор шейдеров, генератор подмены\nтекстур и мастер создания Distant Land."
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
AAFix = "Исправление монохромного фона, возникающего во время загрузки или сохранения при использовании сглаживания."
About = "Показывает общую информацию о программе."
AltCombat = "Позволяет использовать стиль управления боем, применяемый в игре Daggerfall.\nЧтобы использовать эту опцию вы должны связать функцию 'toggle alt combat' с выбранной клавишей в редакторе команд клавиатуры."
AnisoLevel = "Устанавливает уровень анизотропной фильтрации.\nЭта настройка действует только при использовании анизотропной фильтрации для фильтра текстур или фильтра MIP-текстур.\nЕсли вы установите это значение выше поддерживаемого видеокартой, оно будет автоматически снижено до наивысшего поддерживаемого уровня."
AntiAlias = "Устанавливает уровень сглаживания.\nУстановка этого значения выше поддерживаемого видеокартой вызовет ошибку визуализации при запуске Morrowind.\nЕсли вы установите значение, отличное от None, вы не сможете создать блокируемый задний буфер."
Aspect = "Это пропорции, рассчитанные для выбранного разрешения экрана."
AspectZoom = "Установка этой галочки включит коррекцию пропорций изображения при использовании масштабирования."
Audio = "Изменяет значение строки 'Disable Audio'.\nУстановите эту галочку, чтобы отключить большинство звуковых эффектов Morrowind.\nМузыка этой настройкой не отключается."
BindAI = "Установка этой галочки свяжет дистанцию AI с дистанцией обзора.\nУвеличение дальности видимости при помощи MGE автоматически увеличит дистанцию AI."
CalcRefresh = "Находит допустимую частоту обновления для выбранного разрешения экрана.\nДопустимая частота обновления зависит от разрешения экрана, видеокарты и от того, выбран ли 16-битный цветовой режим.\nИзменения, внесенные MGEgui, вызывают автоматический поиск допустимых частот обновления."
CalcResolution = "Щелкните, чтобы изменить разрешение экрана или размер окна Morrowind.\nВ оконном режиме вы можете использовать любой размер окна.\nРазрешения экрана с пропорциями, отличными от 4:3, могут работать неправильно.\nОни отмечены '*'."
Cam3rdPrsn = "Установите эту галочку, чтобы настроить вид от третьего лица.\nПараметры, определяемые этой настройкой, заменят параметры игры по умолчанию при запуске Morrowind.\nНет необходимости устанавливать эту галочку, чтобы изменить положение камеры при помощи команд\nклавиатуры или скрипта. Скрипты могут отключать параметры камеры, переопределённые этой настройкой."
Cam3rdX = "Устанавливает горизонтальный сдвиг положения вида от третьего лица.\nОтрицательные значения перемещают камеру влево, а положительные вправо."
Cam3rdY = "Устанавливает сдвиг положения вида от третьего лица по направлению взгляда.\nЕщё большее уменьшение значений перемещает камеру назад, а увеличение смещает вперёд,\nприближая к игроку. Используются только отрицательные значения потому, что эта настройка, а также\nсоответствующая команда клавиатуры, позволяют размещать вид от третьего лица лишь позади игрока.\nДля установки камеры перед глазами персонажа вы должны создать скрипт, который будет это делать."
Cam3rdZ = "Устанавливает вертикальное смещение положения вида от третьего лица.\nОтрицательные значения смещают камеру вниз, а положительные значения перемещают вверх."
ClearINI = "Установка этой галочки приведет к сбросу сохраненных настроек при сбросе настроек.\nОбратите внимание, что все добавленные комментарии и устаревшие настройки, содержащиеся в файле MGE.ini, будут потеряны."
Console = "Отключает консоль."
DisableMGE = "Отключает подмену интерфейса DirectInput и Direct3D, осуществляемую Morrowind Graphics Extender.\nОбратите внимание, что это не приводит к отключению встроенной версии MWSE, поэтому\nнет необходимости в установке и запуске внешней версии Morrowind Script Extender."
DisableMWSE = "Установка этой галочки отключает функции MWSE, встроенные в MGE.\nЭто позволяет запускать MGE с внешней версией MWSE. Обратите внимание,\nчто некоторые дополнения к игре, использующие функции MGE, требуют\nвстроенную версию MWSE и не будут работать с внешними версиями."
DisplayMessages = "Позволяет MGE отображать текст на экране.\nЭто необходимо для отображения счетчика FPS и других сообщений MGE об изменении различных настроек графики."
DistantLandWizard = "Запускает процесс, создающий файлы, необходимые для работы Distant Land.\nВы не можете использовать Distant Land без предварительного запуска этого мастера."
DLAutoByAFogEnd = "Позволяет автоматически устанавливать остальные расстояния на основе значения конца тумана на земле."
DLAutoByDrawDist = "Позволяет автоматически устанавливать остальные расстояния на основе дальности области прорисовки."
DLAutoDist = "Эта галочка позволяет автоматически устанавливать остальные расстояния в зависимости от выбранного базового расстояния.\nТаким образом, вы не должны думать о таких вещах, как диапазоны тумана, дальность видимости ландшафта или дальних объектов, разве только если вы сами действительно не захотите этого."
DLDistantLand = "Установите эту галочку, чтобы включить функции Distant Land.\nЭто позволит отображать ландшафт и объекты за пределами обычной области видимости Morrowind.\nОбратите внимание, что при этом пиксельные тени игры Morrowind, используемые для отображения\nповерхности воды, будут отключены, и вместо них будет использоваться шейдер воды Distant Land.\nПиксельные тени игры Morrowind будут вновь включены, если вы в MGEgui отключите Distant Land или MGE."
DLDistantStatics = "Позволяет отображать далекие объекты, такие как деревья и здания,\nза пределами обычной области видимости Morrowind."
DLDistFar = "Это расстояние, измеряемое количеством ячеек игры, вплоть до которого должны быть видны объекты 'средних размеров'."
DLDistNear = "Это расстояние, измеряемое количеством ячеек игры, вплоть до которого должны быть видны объекты 'малых размеров'."
DLDistVeryFar = "Это расстояние, измеряемое количеством ячеек игры, вплоть до которого должны быть видны объекты 'больших размеров'."
DLDrawDist = "Это максимальная дальность прорисовки Distant Land, измеряемая количеством ячеек игры.\nВсе, что находится за пределами этого расстояния, не будет отображаться."
